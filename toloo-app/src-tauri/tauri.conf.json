{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Toloo",
  "version": "0.1.0",
  "identifier": "com.irandawn.toloo",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Toloo",
        "width": 1100,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": []
  }
}
